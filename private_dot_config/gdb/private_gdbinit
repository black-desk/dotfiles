python
import sys
import os
kdev_path = '/usr/share/kdevgdb/printers'
if os.path.exists(kdev_path):
    sys.path.insert(0, kdev_path)
    from qt import register_qt_printers
    register_qt_printers(None)
end

set print pretty on
set history save on
set debuginfod enabled on
set environment ASAN_OPTIONS detect_leaks=0
